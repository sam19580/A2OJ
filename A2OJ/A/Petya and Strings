#include<bits/stdc++.h>

using namespace std;
int main()
{
 string s1,s2;
 int k=0;
 ios_base::sync_with_stdio(false);
 cin.tie(0);
cin>>s1>>s2;
transform(s1.begin(),s1.end(),s1.begin(),::tolower);
transform(s2.begin(),s2.end(),s2.begin(),::tolower);
 int n = s1.compare(s2);
 if(n==0){
    cout<<"0"<<"\n";
 }else if(n>0){
  cout<<"1"<<"\n";
  }else{
  cout<<"-1"<<"\n";}

    }
