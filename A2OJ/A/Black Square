
#include <bits/stdc++.h>
using namespace std;
 
 
int main() {
int a=0,b=0,c=0,d=0;
int a1,b1,c1,d1;
cin>>a1>>b1>>c1>>d1;
string s;
cin>>s;
for(int i=0;i<s.length();i++){
    if(s.at(i)=='1'){
        a++;
    }
   else if(s.at(i)=='2'){
    b++;
   }else if(s.at(i)=='3'){
   c++;}else {
   d++;}
}
cout<<a1*a+b1*b+c1*c+d1*d;
}
