#pragma GCC optimize("Ofast")
#include <bits/stdc++.h>
using namespace std;


int main() {
int n;
int arr1[100],arr2[100];
cin>>n;
for(int i=0;i<n;i++){
    cin>>arr1[i]>>arr2[i];
}

int sum=0;
for(int i=0;i<n;i++){
    for(int j=0;j<n;j++){
        if(arr1[i]==arr2[j]){
          sum++;
        }
    }
}
cout<<sum;

}
